    "@storybook/react": "^6.3.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "dtsgenerator": "^3.12.1",
    "enzyme": "^3.11.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "storybook": "^6.3.1"
  }

    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^24.2.0",
    "dtsgenerator": "^3.19.2",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",

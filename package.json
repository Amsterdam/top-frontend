    "@types/node": "^24.2.0",
    "@types/qs": "^6.9.18",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^5.0.3",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.4",
    "env-cmd": "^10.1.0",
    "eslint-config-react-app": "^7.0.1",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "immer": "^10.1.1",
    "jwt-decode": "^4.0.0",
    "lodash.difference": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "qs": "^6.14.0",
